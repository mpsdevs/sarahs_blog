<p id="notice"><%= notice %></p>


<h1>Events</h1>



<div clas="row">
  <div class="col-lg-12">
  <table>
    <thead>
      <tr>
        <th class="th">Title</th>
        <th class="th">Start Time</th>
        <th class="th">Description</th>
        <th class="th">Address</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td class="th"><%= event.title %></td>
          <td class="th"><%= event.start_time %></td>
          <td class="th"><%= event.description %></td>
          <td class="th"><%= event.address %></td>

          <td class="th"><%= link_to 'Show', event %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>
<br>


<div><%= button_to 'New Event', new_event_path, method: :get, :class => "btn btn-info" %></div>
